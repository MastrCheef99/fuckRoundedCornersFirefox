{
    "manifest_version": 3,
    "name": "Fuck Rounded Corners",
    "version": "1.1",
    "description": "Changes the roundness of all corners based on user preferance",
     "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icon.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "{BCD48F10-DCE0-4413-9781-955B10F261A4}",
        "strict_min_version": "109.0"
      }
    }
  }