{
    "manifest_version": 3,
    "name": "Fuck Rounded Corners",
    "version": "2",
    "description": "Changes the roundness of all corners based on user preferance",
     "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icon.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "fuckroundedcorners@example.com",
        "strict_min_version": "109.0"
      }
    }
  }